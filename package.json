{
  "name": "ts_connect",
  "version": "1.0.0",
  "author": "Alexey Sulimenko sulimenkoas@gmail.com>",
  "description": "Connector for TradeStation API",
  "contributors": [
    "Alexey Sulimenko <sulimenkoas@gmail.com>"
  ],
  "license": "UNLICENSED",
  "private": true,
  "keywords": [
    "trading",
    "api",
    "connector",
    "tradingstation",
    "websocket",
    "rpc",
    "internal",
    "proprietary"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sulimenko/ts_connect"
  },
  "bugs": {
    "url": "https://github.com/sulimenko/ts_connect/issues"
  },
  "homepage": "https://github.com/sulimenko/ts_connect",
  "main": "server.js",
  "types": "types/global.d.ts",
  "scripts": {
    "test": "npm run lint && npm run types && MODE=test node server.js",
    "dotest": "npm run lint && npm run types && node server.js",
    "types": "tsc -p tsconfig.json",
    "lint": "eslint . && prettier -c \"**/*.js\" \"**/*.json\" \"**/*.md\"",
    "fix": "eslint . --fix && prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\"",
    "db": "metasql c",
    "start": "node server.js"
  },
  "engines": {
    "node": "18 || 20 || 21 || 22"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/ws": "^8.18.1",
    "eslint": "^9.36.0",
    "eslint-config-metarhia": "^9.1.3",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "impress": "^3.1.1",
    "redis": "^5.8.2"
  }
}
